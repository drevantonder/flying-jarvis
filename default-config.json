{
  "agents": {
    "defaults": {
      "model": {
        "primary": "opencode/claude-opus-4-6",
        "fallbacks": ["anthropic/claude-opus-4-5", "anthropic/claude-sonnet-4-5", "openai/gpt-5.2", "openai/gpt-4o"]
      },
      "maxConcurrent": 4,
      "workspace": "/data/workspace"
    },
    "list": [
      {
        "id": "main",
        "default": true
      },
      {
        "id": "hooks"
      }
    ]
  },
  "auth": {
    "profiles": {
      "opencode:default": { "mode": "token", "provider": "opencode" },
      "anthropic:default": { "mode": "token", "provider": "anthropic" },
      "openai:default": { "mode": "token", "provider": "openai" }
    }
  },
  "gateway": {
    "mode": "local",
    "bind": "auto",
    "port": 3000
  },
  "hooks": {
    "enabled": false,
    "path": "/hooks",
    "allowedAgentIds": ["*"]
  },
  "meta": {
    "lastTouchedVersion": "2026.1.29"
  }
}
